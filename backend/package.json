{
  "name": "area-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start -p 8080",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "deploy-discord-commands": "tsx src/services/discord/deploy-commands.ts",
    "docker:build": "docker build -t area-backend .",
    "docker:dev": "docker build -f Dockerfile.dev -t area-backend-dev .",
    "docker:run": "docker run -p 8080:8080 --env-file ../.env area-backend"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "jsonwebtoken": "^9.0.2",
    "next": "14.0.4",
    "node-cron": "^3.0.3",
    "react": "^18",
    "react-dom": "^18",
    "resend": "^3.2.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.7",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "typescript": "^5"
  }
}
